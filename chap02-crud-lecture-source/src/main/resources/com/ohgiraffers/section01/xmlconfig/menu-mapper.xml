<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper>
    <select id="selectAllMenus">
        SELECT
                MENU_CODE
              , MENU_NAME
              , MENU_PRICE
              , CATEGORY_CODE
              , ORDERABLE_STATUS
        FROM tbl_menu
    </select>
</mapper>